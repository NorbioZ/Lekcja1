package com.company;
import java.util.Scanner;

import static java.lang.Math.*;
public class Main {


    public static void main(String[]args)

    {


        Scanner scanner = new Scanner(System.in);

        double x1, x2, y1, y2;

        System.out.println("Wpisz współrzędne");

        x1 = scanner.nextInt();

        y1 = scanner.nextInt();

        x2 = scanner.nextInt();

        y2 = scanner.nextInt();

        System.out.println("Wynik to " + sqrt((Math.pow(x2-x1, 2)) + (Math.pow(y2-y1, 2))));
    //Norbert Kulik

    }
}
