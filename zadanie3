package com.company;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        System.out.println(" Ile stopni Celsjusza?");
        int stopnie = scanner.nextInt();
        System.out.println(" Zamieniam na Fahrenheita");
        System.out.println ( (stopnie * 1.8) + 32);
    
    }
}
